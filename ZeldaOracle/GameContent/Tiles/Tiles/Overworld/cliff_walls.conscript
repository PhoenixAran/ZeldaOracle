TILE "cliff_tunnel_keyhole_down";
	BASICLEDGE	block, down;
	RESETWHEN	Never;
	TILEBELOW	"cliff_tunnel_down";
	SAMESPRITE;
END;
TILE "cliff_tunnel_keyhole_up";
	BASICLEDGE	block, up;
	RESETWHEN	Never;
	TILEBELOW	"cliff_tunnel_up";
	SAMESPRITE;
END;
TILE "cliff_tunnel_keyhole_left";
	BASICLEDGE	block, left;
	RESETWHEN	Never;
