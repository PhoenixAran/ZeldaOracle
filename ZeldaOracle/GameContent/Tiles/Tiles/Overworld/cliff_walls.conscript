	SAMESPRITE;
END;
TILE "cliff_tunnel_keyhole_right";
	BASICLEDGE	block, right;
	RESETWHEN	Never;
	TILEBELOW	"cliff_tunnel_right";
	SAMESPRITE;
END;

TILE "temp_cliff_bombable";
	SOLID		block;
	FLAGS		Bombable;
	RESETWHEN	Never;
	BREAKANIM	"effect_rock_break";
	BREAKSOUND	"rock_shatter";
END;

TILE "cliff_bombable_down";
	CLONE		"temp_cliff_bombable";
	BASICLEDGE	block, down;
	TILEBELOW	"cliff_tunnel_down";
	SAMESPRITE;
END;
TILE "cliff_bombable_up";
